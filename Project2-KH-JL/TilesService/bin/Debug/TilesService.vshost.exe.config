<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <system.serviceModel>
    <services>
      <service name="TilesLibrary.TileBag">
        <endpoint name="TileBag"
                  address="net.tcp://localhost:10000/TilesLibrary/TileBag"
                  binding="netTcpBinding"
                  bindingConfiguration="myBindingConfig"
                  contract="TilesLibrary.ITileBag"/>
      </service>
    </services>
    <bindings>
      <netTcpBinding>
        <binding name="myBindingConfig">
          <security mode="None"/>
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
</configuration>